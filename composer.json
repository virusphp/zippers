{
  "name": "virusphp/zipper",
  "type": "library",
  "description": "This is a little neat helper for the ZipArchive methods with handy functions",
  "keywords": [
    "laravel",
    "ZIP",
    "Archive"
  ],
  "homepage": "http://github.com/virusphp/zipper",
  "license": "MIT",
  "authors": [
    {
      "name": "Sugandi",
      "email": "github@setsuga.io",
      "homepage": "http://megonodev.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0|^7.4|^8.0",
    "ext-zip": "*",
    "illuminate/support": "^8.8|^9.1.0",
    "illuminate/filesystem": "^8.8|^9.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.10",
    "mockery/mockery": "^1.4.4"
  },
  "autoload": {
    "psr-4": {
      "Virusphp\\Zipper\\": "src/Virusphp/Zipper"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Virusphp\\Zipper\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Virusphp\\Zipper\\ZipperServiceProvider"
      ],
      "aliases": {
        "Zipper": "Virusphp\\Zipper\\Zipper"
      }
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
